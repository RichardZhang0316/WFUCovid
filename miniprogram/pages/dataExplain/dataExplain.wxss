page{
    height: 100%;
    /* background-image: linear-gradient(to bottom, #9E7E38 0%,#9E7E38 20%, #ececec 20%,#ececec 100%); */
    background-color: #ececec;
} 
.topTitle{
    /*卡片的宽度*/  
    width: 90%;
    text-align: center;
    /*左外边距，这里代表距离最左边的距离*/
    margin-left: 5%;
    /*卡片的高度。*/
    height: 150rpx;
    padding-top: 30px;
    font-size: 20px;
    color:rgb(158, 126, 56);
}
.card{
     /*卡片的宽度*/  
     color:rgb(158, 126, 56);
     width: 85%;
     /*左外边距，这里代表距离最左边的距离*/
     margin-right:5%;
     margin-left: 5%;
     /*卡片的高度。*/
     height:auto;
     padding: 10px;
     /*背景色，可以自己更换*/
     background: rgb(255, 255, 255);
     border-radius:10px;
     /* radius是卡片的边框的圆润c程度*/
     margin-bottom: 30px;
     /* box-shadow: 5px 5px 5px #FDC314; */
     letter-spacing: 0.2em;
}
.Pro{
    font-style: italic;
    color:rgb(196, 196, 196);
    text-align: center;
    padding-bottom: 30px;
}